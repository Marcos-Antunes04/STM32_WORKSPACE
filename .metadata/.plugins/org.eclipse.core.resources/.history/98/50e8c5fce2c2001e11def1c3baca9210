/*
Assembly para ARM -> ANTUNES
*/

.syntax unified
.thumb
.section .text
.cpu cortex-m4

.equ RCC_AHB1ENR, 	0x40023830
.equ GPIOD_EN, 		(1<<3)

.equ GPIOD_MODER,	0x40020C00
.equ OUTPUT,		(1<<14)

.equ GPIOD_ODR,		0x40020C14
.equ LED_ON,		(1<<14)

.global main
main:

ldr r0, =RCC_AHB1ENR
ldr r1, [r0]
orr r1, #GPIOD_EN
str r1,[r0]

ldr r0, =GPIOD_MODER
ldr r1, [r0]
orr r1, #OUTPUT
str r1, [r0]

.end
