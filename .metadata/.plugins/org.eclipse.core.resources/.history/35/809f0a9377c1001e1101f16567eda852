#include "main.h"

int main(void){
	RCC->CR |= (1 << 16); //enables HSE
	while(!(RCC->CR & (1 << 17))); //waits for HSE to become ready
	RCC->APB1ENR |= (1 << 28); //Power enable clock
	PWR->CR |= (3 << 14); //Voltage regulator Scale 1. (DEFAULT)

	FLASH->ACR = (1 << 8) | (1 << 9) | (1 << 10) | (5 << 0);
	while (1){

	}
}
